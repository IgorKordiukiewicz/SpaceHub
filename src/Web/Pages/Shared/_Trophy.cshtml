@model int

@{
    if(Model == 1)
    {
        <text>(<i class="bi bi-trophy-fill" style="color:#D6AF36;"></i>)</text>
    }
    else if(Model == 2)
    {
        <text>(<i class="bi bi-trophy-fill" style="color:#A7A7AD;"></i>)</text>
    }
    else if(Model == 3)
    {
        <text>(<i class="bi bi-trophy-fill" style="color:#A77044;"></i>)</text>
    }
}
